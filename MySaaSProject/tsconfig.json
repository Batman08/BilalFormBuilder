{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "moduleResolution": "node",
    "target": "ES6",
    "outDir": "wwwroot/typescript"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "wwwroot/typescript/**/**.ts"
  ]
}